  "version": "0.2.0",
  "configurations": [
    {
      "name": "auth mode",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}",
      "args": [ "-auth", "-port", "8080" ]
    },
    {
      "name": "resource mode",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}",
      "args": [ "-resource", "-port", "9090" ]
    },
    {
      "name": "full mode",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}",
      "args": [ "-auth", "-resource", "-port", "8080" ]
    }
  ]
}